{
  "Django Simple Form ":{
    "prefix": "sform",
    "body": [
      "class ${1:Name}Form(forms.Form):",
      "\t# Define $1Form fields here",
      "\t${0}"
    ],
    "description": "Django Simple Form"
  },
  "Django Model Form ":{
    "prefix": "modelform",
    "body": [
      "class ${1:ModelName}Form(forms.ModelForm):",
      "\tclass Meta:",
      "\t\tmodel = ${2:$1}",
      "\t\tfields = (${3:\"__all__\"})",
      "\t\t${0}"
    ],
    "description": "Django Model Form"
  },
